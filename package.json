{
  "name": "meteo-station-domotique",
  "version": "4.2.10",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "start": "ng serve --open --aot --port=8080",
    "test": "jest --clearCache && jest --showConfig && jest --ci",
    "build-doc": "compodoc -p tsconfig.json -d ./documentation/compodoc ",
    "load-doc": "compodoc -p tsconfig.json -d ./documentation/compodoc -o -s --port 8082",
    "build": "ng build --prod --baseHref=./ --outputPath=docs/meteo-station",
    "build-test": "npm run build & http-server -p 8081 -c-1 -o docs/meteo-station",
    "build-all": "npm run test & npm run build & npm run build-doc",
    "build-all-deploy": "npm run build-all & npm run deploy",
    "build-all-deploy-prod": "npm run build-all & npm run deploy-prod",
    "deploy": "ts-node ./deployment/deploy.ts",
    "deploy-prod": "ts-node ./deployment/deploy.ts --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.8",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.8",
    "@angular/compiler": "^10.0.8",
    "@angular/core": "^10.0.8",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^10.0.8",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.8",
    "@angular/platform-browser-dynamic": "^10.0.8",
    "@angular/router": "^10.0.8",
    "@angular/service-worker": "^10.0.8",
    "@auth0/angular-jwt": "^5.0.1",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-google-charts": "^1.1.4",
    "bootstrap": "^4.5.2",
    "firebase": "^7.17.2",
    "jquery": "^3.5.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-select-ex": "^4.0.0",
    "ngx-spinner": "^10.0.1",
    "ngx-toastr": "^13.0.0",
    "popper.js": "^1.16.0",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "^10.0.8",
    "@angular/language-service": "^10.0.8",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "ftp-deploy": "^2.3.7",
    "http-server": "^0.12.3",
    "jest": "^26.3.0",
    "jest-html-reporter": "^3.1.3",
    "jest-preset-angular": "^8.2.1",
    "minimist": "^1.2.5",
    "promise-ftp-common": "^1.1.5",
    "protractor": "~7.0.0",
    "readline-sync": "^1.4.10",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7"
  }
}
