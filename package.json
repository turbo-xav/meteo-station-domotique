{
    "name": "meteo-station-domotique",
    "description": "Station Météo connectée",
    "title": "Station Météo",
    "date": "2021-02-26",
    "version": "1.1.0",
    "scripts": {
        "ng": "ng",
        "postinstall": "ngcc",
        "start": "ng serve --hmr --open --port=8080",
        "test": "jest --clearCache && jest --ci",
        "prebuild": "ts-node ./deployment/deploy-date.ts",
        "build": "ng build --prod --baseHref=./",
        "build:stats": "npm run build -- --stats-json && webpack-bundle-analyzer  ./docs/meteo-station/stats.json",
        "deploy": "ts-node ./deployment/deploy.ts",
        "build:open": "npm run build && http-server ./docs/meteo-station -p 8081 -c-1 -o ",
        "build:deploy": "npm run build && npm run deploy",
        "build:deploy:prod": "npm run build && npm run deploy:prod",
        "deploy:prod": "npm run deploy -- --prod",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^11.0.9",
        "@angular/cdk": "^11.0.4",
        "@angular/common": "^11.0.9",
        "@angular/compiler": "^11.0.9",
        "@angular/core": "^11.0.9",
        "@angular/forms": "^11.0.9",
        "@angular/material": "^11.0.4",
        "@angular/platform-browser": "^11.0.9",
        "@angular/platform-browser-dynamic": "^11.0.9",
        "@angular/router": "^11.0.9",
        "@angular/service-worker": "^11.0.9",
        "@auth0/angular-jwt": "^5.0.2",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "angular-google-charts": "^2.0.0",
        "readline-sync": "^1.4.10",
        "rxjs": "^6.6.3",
        "tslib": "^2.1.0",
        "zone.js": "^0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1100.7",
        "@angular/cli": "^11.0.7",
        "@angular/compiler-cli": "^11.0.9",
        "@compodoc/compodoc": "^1.1.11",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.21",
        "codelyzer": "^6.0.1",
        "ftp-deploy": "^2.4.0",
        "http-server": "^0.12.3",
        "jest": "^26.6.3",
        "jest-html-reporter": "^3.3.0",
        "jest-preset-angular": "^8.3.2",
        "promise-ftp-common": "^1.1.5",
        "protractor": "~7.0.0",
        "ts-node": "^9.1.1",
        "tslint": "~6.1.0",
        "typescript": "~4.0.5",
        "webpack-bundle-analyzer": "^4.3.0"
    }
}